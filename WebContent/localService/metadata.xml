<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<edmx:Edmx Version="1.0"
	xmlns="http://schemas.microsoft.com/ado/2008/09/edm"
	xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
	xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata"
	xmlns:edmanno="http://schemas.microsoft.com/ado/2009/02/edm/annotation"
	xmlns:sap="http://www.sap.com/Protocols/SAPData">

	<edmx:DataServices m:DataServiceVersion="2.0">
		<Schema Namespace="DemoModel" xml:lang="en">

			<EntityType Name="Node">
				<Key>
					<PropertyRef Name="NodeID" />
				</Key>
				<Property Name="NodeID" Type="Edm.String" Nullable="false" Unicode="true" edmanno:StoreGeneratedPattern="Identity" />
				<Property Name="HierarchyLevel" Type="Edm.Int32" Nullable="false" />
				<Property Name="Description" Type="Edm.String" Nullable="false" MaxLength="40" Unicode="true" FixedLength="false" />
				<Property Name="ParentNodeID" Type="Edm.String" Nullable="true" Unicode="true"/>
				<Property Name="DrillState" Type="Edm.String" Nullable="false" MaxLength="10" Unicode="true" FixedLength="false" />
			</EntityType>
			
			<EntityType Name="Product">
				<Key>
					<PropertyRef Name="CODE" />
				</Key>
				<Property Name="Name" Type="Edm.String" Nullable="false" />
				<Property Name="CODE" Type="Edm.String" Nullable="false" edmanno:StoreGeneratedPattern="Identity" />
				<Property Name="PRICE" Type="Edm.Int32" Nullable="true"/>
				<Property Name="DATE" Type="Edm.Int32" Nullable="false" />
			</EntityType>
			
			<EntityType Name="SideMenu">
				<Key>
					<PropertyRef Name="CODE" />
				</Key>
				<Property Name="Name" Type="Edm.String" Nullable="false" />
				<Property Name="CODE" Type="Edm.String" Nullable="false"/>
				<Property Name="PRICE" Type="Edm.Int32" Nullable="true"/>
				<Property Name="DATE" Type="Edm.Int32" Nullable="false" />
			</EntityType>

			<EntityContainer Name="DemoEntities" 
							edmanno:LazyLoadingEnabled="true" 
							sap:supported-formats="atom json"
							m:IsDefaultEntityContainer="true">
				<EntitySet Name="Nodes" EntityType="DemoModel.Node" />
				<EntitySet Name="Products" EntityType="DemoModel.Product" />
				<EntitySet Name="SideMenues" EntityType="DemoModel.SideMenu" />
			</EntityContainer>

		</Schema>
	</edmx:DataServices>
</edmx:Edmx>
