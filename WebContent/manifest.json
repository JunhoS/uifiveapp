{
	"_version": "1.8.0",
	"sap.app": {
		"id": "uifiveApp",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "UI5 Template",
		"description": "",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mockServer": {
				"uri": "/here/goes/your/serviceUrl/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "uifiveApp.view.app",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "uifiveApp.i18n.i18n"
				}
			},
			"oData" : {
				"dataSource": "mockServer"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "uifiveApp.view",
				"controlId": "mainApp",
				"controlAggregation": "pages",
				"async": true,
				"transition": "slide"
			},
			"routes": [
				{
					"name": "master",
					"pattern": "",
					"target": ["master", "details"]
				},
				{
					"name": "orderMasters",
					"pattern": "master/:masterName:",
					"target": ["master", "details"]
				},
				{
					"name": "ActionSheet",
					"pattern": "ActionSheet",
					"target": ["master", "ActionSheet"]
				},
				{
					"name": "AddBookmarkButton",
					"pattern": "AddBookmarkButton",
					"target": ["master", "AddBookmarkButton"]
				},
				{
					"name": "Breadcrumbs",
					"pattern": "Breadcrumbs",
					"target": ["master", "Breadcrumbs"]
				}
			],
			"targets": {
				"master": {
					"viewName": "sidemenu",
					"controlAggregation": "masterPages",
					"viewLevel": 0
				},
				"details": {
					"viewName": "detailPage",
					"controlAggregation": "detailPages",
					"viewLevel": 0
				},
				"ActionSheet" : {
					"viewName": "Action.ActionSheet",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				},
				"AddBookmarkButton" : {
					"viewName": "Action.AddBookmarkButton",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				},
				"Breadcrumbs" : {
					"viewName": "Action.Breadcrumbs",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				}
			}
		}
	}
}