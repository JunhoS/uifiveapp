{
	"sap.ui5": {
		"rootView": {
			"viewName": "uifiveApp.view.app",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"libs": {
				"sap.m": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "uifiveApp.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "uifiveApp.view",
				"controlId": "mainApp",
				"controlAggregation": "pages",
				"async": true,
				"transition": "slide"
			},
			"routes": [
				{
					"name": "master",
					"pattern": "",
					"target": ["master", "details"]
				},
				{
					"name": "orderDetails",
					"pattern": "orders/:orderId:",
					"target": ["master", "details"]
				}
			],
			"targets": {
				"master": {
					"viewName": "sidemenu",
					"controlAggregation": "masterPages",
					"viewLevel": 0
				},
				"details": {
					"viewName": "detailPage",
					"controlAggregation": "detailPages",
					"viewLevel": 1
				}
			}
		}
	}
}