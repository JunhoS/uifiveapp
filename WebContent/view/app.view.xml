<mvc:View
	controllerName="uifiveApp.controller.app"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:tnt="sap.tnt"
	height="100%">
	<tnt:ToolHeader id="mainheader">
		<Button icon="sap-icon://menu2" type="Transparent" press="onMenuOpen">
			<layoutData>
				<OverflowToolbarLayoutData priority="NeverOverflow" />
			</layoutData>
		</Button>
		<ToolbarSpacer width="20px" />
	</tnt:ToolHeader>
	<SplitApp id="mainApp"/>
</mvc:View>